<%- include ../partials/header.ejs %>
 
 
<%- include ../partials/main_nav.ejs  %>

  
  <!--Main Content--> 
 

  <main class="container">
    <div class="row">
        <div class="offset-md-3 col-md-6">
            <h1><%= title%></h1>

            <form class="form" method="POST">
                <div class="form-group">
                    <label for="nameTextField">Name</label>
                    <input type="text" class="form-control"
                    id="nameTextField"
                    placeholder="Enter Contact Name"
                    name="name" required>
                </div>

                <div class="form-group">
                    <label for="contact_numberTextField">Contact_Number</label>
                    <input type="text" class="form-control"
                    id="contact_numberTextField"
                    placeholder="Enter Contact Number"
                    name="contact_number" required>
                </div>

                <div class="form-group">
                    <label for="emailTextField">Email Id</label>
                    <input type="text" class="form-control"
                    id="emailTextField"
                    placeholder="Enter Email Id"
                    name="email" required>
                </div>
                <button class="btn btn-primary" type="submit">
                    <i class="fas fa-edit"></i>Submit</button>

                    <a href="/business-list" class="btn btn-warning">
                        <i class="fas fa-undo"></i> Cancel</a>


            </form>
        </div>
    </div>
    </main>




<%- include ../partials/bottom_nav.ejs %>


<%- include ../partials/footer.ejs %>